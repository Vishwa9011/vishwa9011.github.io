{
    "plugins": ["prettier-plugin-tailwindcss"],
    "tailwindFunctions": ["clsx"],
    "tailwindPreserveWhitespace": true,
    "tabWidth": 4,
    "useTabs": false,
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid",
    "htmlWhitespaceSensitivity": "strict",
    "singleAttributePerLine": false
}
